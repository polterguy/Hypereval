/*
 * Desktop widget.
 */
container
  element:a
  class:desktop-app shaded rounded air-inner
  title:Hypereval is a 'terminal' type of app, allowing you to evaluate arbitrary Hyperlambda
  widgets
    span
      class:desktop-app-name
      widgets
        literal
          element:span
          class:icon-bolt desktop-app-icon
        span
          class:desktop-app-text
          innerValue:Hypereval
